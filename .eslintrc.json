{
  "extends": [
    "react-app",
    "react-app/jest"
  ],
  "rules": {
    "array-callback-return": "warn",
    "no-unused-vars": "off",
    "no-console": "off",
    "react-hooks/exhaustive-deps": "warn",
    "no-unused-expressions": "off"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
} 